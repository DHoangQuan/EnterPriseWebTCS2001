@model IEnumerable<EnterPriseWeb.Models.content>

@{
    ViewBag.Title = "Index";

}

<h2>Index</h2>


@if (Session["abc"].ToString() == "1")
{

    <p>
        <a href="@Url.Action("create", "contents")" class="btn btn-success">Add new post</a>
    </p>
}
else if (Session["abc"].ToString() == "2")
{
    <h3>out of date to upload magazine but student still can edit your upload until final closure date</h3>
}










@foreach (var item in Model)
{
    @Html.Raw("<div class=\"post\" style='padding: 20px; width: 1000px;'>" + "<ul style='list-style-type: none; background-color: #F1F1F1'>"
    + "<li><h3>" + item.account.account_name + "</h3></li>"
    + "<li><h3>" + item.content_id + "</h3></li>"
    + "<li><h3>" + item.content_title+ "<h3></li>"
    + "<li>"+ item.content_post + "</li></ul>" + "</div>"


       )
}
