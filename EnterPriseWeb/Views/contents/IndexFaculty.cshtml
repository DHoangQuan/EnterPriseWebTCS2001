@model IEnumerable<EnterPriseWeb.Models.content>
@{
    ViewBag.Title = "Index of IT Faculty";
    Layout = "~/Views/Shared/IndexBlog.cshtml";
}

     
 
@foreach (var item in Model)
{
    @Html.Raw("<div class=\"post\" style='padding: 20px; width: 1000px;'>" + "<ul style='list-style-type: none; background-color: #F1F1F1'>"
                       + "<li>" + item.account.account_name + "</li>"
                       + "<li>" + item.content_id + "</li>"
                       + "<li>" + item.content_title + "</li>"
                       + "<li>" + item.content_post + "</li></ul>" + "</div>"


       )
}
